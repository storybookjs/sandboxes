{
  "name": "angular-v16",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "serve:ssr:angular-v16": "node dist/angular-v16/server/server.mjs"
