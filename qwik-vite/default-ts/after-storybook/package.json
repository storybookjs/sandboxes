{
  "name": "my-qwik-basic-starter",
  "description": "Demo app with sample routes",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.17.2",
    "@builder.io/qwik-city": "^1.17.2",
    "@eslint/js": "latest",
    "@storybook/addon-docs": "^10.0.5",
    "@types/node": "20.19.0",
    "eslint": "9.32.0",
    "eslint-plugin-qwik": "^1.17.2",
    "globals": "16.4.0",
    "prettier": "3.6.2",
    "storybook": "^10.0.5",
    "storybook-framework-qwik": "^0.5.0",
    "typescript": "5.4.5",
    "typescript-eslint": "8.38.0",
    "typescript-plugin-css-modules": "latest",
    "undici": "*",
    "vite": "7.1.11",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "resolutions": {
    "@storybook/addon-a11y": "10.0.5",
    "@storybook/addon-docs": "10.0.5",
    "@storybook/addon-links": "10.0.5",
    "@storybook/addon-onboarding": "10.0.5",
    "storybook-addon-pseudo-states": "10.0.5",
    "@storybook/addon-themes": "10.0.5",
    "@storybook/addon-vitest": "10.0.5",
    "@storybook/builder-vite": "10.0.5",
    "@storybook/builder-webpack5": "10.0.5",
    "storybook": "10.0.5",
    "@storybook/angular": "10.0.5",
    "@storybook/ember": "10.0.5",
    "@storybook/html-vite": "10.0.5",
    "@storybook/nextjs": "10.0.5",
    "@storybook/nextjs-vite": "10.0.5",
    "@storybook/preact-vite": "10.0.5",
    "@storybook/react-native-web-vite": "10.0.5",
    "@storybook/react-vite": "10.0.5",
    "@storybook/react-webpack5": "10.0.5",
    "@storybook/server-webpack5": "10.0.5",
    "@storybook/svelte-vite": "10.0.5",
    "@storybook/sveltekit": "10.0.5",
    "@storybook/vue3-vite": "10.0.5",
    "@storybook/web-components-vite": "10.0.5",
    "sb": "10.0.5",
